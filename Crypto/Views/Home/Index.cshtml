@{
    ViewData["Title"] = "Home Page";
}
<script src="https://code.jquery.com/jquery-3.5.1.js"></script>
<div class="row">
    <div class="col">
        <div class="small-block">
            <!----> <a href="/trade/inverse/BTCUSD" id="block" style="text-decoration:none;">
                <div class="small-block__container">
                    <div class="small-block__content">
                        <p brand="" class="small-block__name" id="from-to">BTC-USD</p>
                        <div class="small-block__current">
                            <p class="small-block__current-rate" id="rate">
                            </p>
                        </div>
                    </div> <p class="small-block__value" id="live-value"></p>

                    <div class="small-block__label-bg">
                        <div class="small-block__label"><span>24hr Volume</span> <span class="24volClss" id="24volBTC"></span></div>
                        <div class="small-block__label"><span>Market Capitalization</span> <span class="mkCapClss" id="MKTCAP"></span></div>
                    </div>
                </div>
            </a>
        </div>

    </div>

    <div class="col">
