        <thead>
            <tr>
                <th>#</th>
                <th>Name</th>
                <th></th>
                <th>Price</th>
                <th>MarketCap</th>
                <th>24H</th>
                <th>Volume</th>
                <th>TotalSupply</th>
            </tr>
        </thead>
    </table>
</div>
